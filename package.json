{
  "name": "itlab-test",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "pre-commit": "npm-run-all lint format test",
    "lint": "tslint -t codeFrame --project ./tsconfig.json --fix 'src/**/*.ts' 'test/**/*.ts'",
    "format": "prettier --write \"{config,migrations,models,seeders,src}/**/*.{ts,js}\"",
    "test": "jest",
    "test-watch": "jest --watch",
    "build": "webpack",
    "watch:build": "webpack -w",
    "start": "node ./dist/server.js",
    "watch:server": "nodemon -w ./dist/server.js --exec node ./dist/server.js",
    "db:setup": "npm-run-all db:create db:migrate db:seed",
    "db:create": "sequelize db:create",
    "db:migrate": "sequelize db:migrate",
    "db:drop": "sequelize db:drop",
    "db:seed": "sequelize db:seed:all"
  },
  "devDependencies": {
    "@types/jest": "^23.3.11",
    "husky": "^1.0.0-rc.2",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3"
  },
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@types/express": "^4.16.0",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^6.0.12",
    "@types/redux": "^3.6.0",
    "@types/sequelize": "^4.27.33",
    "babel-loader": "^8.0.5",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "html-webpack-plugin": "^3.2.0",
    "npm-run-all": "^4.1.5",
    "pg": "^7.7.1",
    "pg-hstore": "^2.3.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "reflect-metadata": "^0.1.12",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0",
    "sequelize-typescript": "^0.6.6",
    "ts-loader": "^5.3.2",
    "tsconfig-paths": "^3.7.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-microsoft-contrib": "^6.0.0",
    "typescript": "^3.2.2",
    "uuid": "^3.3.2",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.0",
    "webpack-node-externals": "^1.7.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
